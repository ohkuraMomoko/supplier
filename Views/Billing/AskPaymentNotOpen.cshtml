@{
    ViewBag.Title = "尚未開通";
}

@section header {
    @Styles.Render("~/Content/pages/case/notOpen.min.css")
}

<div class="page">
    <div class="msg">
        <img src="@Url.Content("~/Content/images/icon-confirm-none-authorize.svg")" alt="" class="center msg--pic">
        <h3 class="msg--title">你尚未開通此功能</h3>
        <p class="msg--desc">
            1.	您可針對帳號內可看見且屬於【線上填單、零卡掃碼】的訂單，發送請款。
            <br>2.	此功能需具有「請款權限」，目前部分商家試行中，如果想瞭解更多，請洽詢仲信業務經辦。
        </p>
    </div>
</div>
@section scripts {
<script type="text/babel">
    var page = new Vue({
        el: "#page",
        created: function() {
			app.currentPage.AskPayment = true
		},
    })
</script>
}