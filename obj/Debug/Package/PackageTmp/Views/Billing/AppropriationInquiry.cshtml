                        const link = document.createElement('a');
                        link.href = url;
                        link.setAttribute('download', 'file.xlsx'); //or any other extension
                        document.body.appendChild(link);
                        link.click();
                    }
                  
                   //匯出明細流程
				   this.toast('已匯出明細')
                }); *@
			},
		}
	});
</script>
